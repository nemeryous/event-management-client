{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.jsx",
    "index.html",
    "src/routes/index.jsx"
  ],
  "project": [
    "src/**/*.{js,jsx,ts,tsx}"
  ],
  "ignore": [
    "eslint.config.js",
    "postcss.config.js",
    "tailwind.config.js",
    ".github/**",
    "dist/**",
    "public/**"
  ],
  "ignoreDependencies": [
    "eslint-plugin-react-refresh",
    "@tailwindcss/vite"
  ],
  "paths": {
    "@/*": ["src/*"],
    "@components/*": ["src/components/*"],
    "@pages/*": ["src/pages/*"],
    "@hooks/*": ["src/hooks/*"],
    "@routes/*": ["src/routes/*"],
    "@layouts/*": ["src/layouts/*"],
    "@api/*": ["src/api/*"],
    "@store/*": ["src/store/*"],
    "@utils/*": ["src/utils/*"]
  }
}